{
  "name": "app-with-server-rendering",
  "version": "0.0.1",
  "description": "A basic application that demonstrates serverside rendering",
  "repository": "git@github.com:azaharakis/app-with-server-rendering.git",
  "author": "Alexander Zaharakis <alex.zaharakis@rea-group.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config=./config/webpack.config.js",
    "start": "npm run build && node ./src/app.js",
    "watch": "webpack --watch --config=./config/webpack.config.js",
    "dev": "concurrently 'npm run watch' 'forever --watch ./src/app.js'",
    "lint": "eslint ./src",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "npm run lint && npm run flow"
  },
  "dependencies": {
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.37.4",
    "forever": "^0.15.3",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  }
}
